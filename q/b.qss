/*工作区域右侧不变控制区域*/ 
QPushButton#mSwitchBtn {
    min-width:240px;
    min-height:75px;
}

QPushButton#mSwitchBtn[run=true] {
    border-image:url(":/img/run1.png");
    border-radius:7px;
}

QPushButton#mSwitchBtn[run=false] {
    border-image:url(":/img/stop1.png");
    border-radius:7px;
}
/*工作区域右侧停止、启动标签样式设置*/
#mRunLb,#mStopLb {
    font:16pt; 
}
#mStopLb[run=true] {
    color:rgb(255, 0, 0);
}
#mRunLb[run=true]{
	color:rgb(2, 167, 40); 
}
#mRunLb[run=false],#mStopLb[run=false] {
    color:rgb(68, 68, 68);
}

#mWarnTbl[isValid=true] {
    border-image: url(":/img/sanjiao.png");
}

#mWarnTbl[isValid=false] {
    border-image: url(":/img/sanjiao_f.png");
}

