    background-position:center;
    background-repeat: no-repeat;
    border:none;
}
QPushButton#maximizeButton:hover {
    border-image:url(":/img/big_hover.png");
    background-position:center;
    background-repeat: no-repeat;
    border:none;
}
QPushButton#closeButton {
    border-image:url(":/img/close.png");
